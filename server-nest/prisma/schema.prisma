// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

model Server {
    id        Int    @id @default(autoincrement())
    ipAddress String
    name      String
    memory    String
    type      String
    image     String
    status    String
}
